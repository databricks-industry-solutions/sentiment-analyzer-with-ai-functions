    "displayName" : "reviews_analysis",
    "query" : "SELECT * FROM mido_edw_dev.sentiment_analysis.reviews_analysis"
  }, {
    "name" : "11bb9cee",
    "displayName" : "worse_products",
    "query" : "SELECT\nitem_name,\nsum(review_count) Total_Reviews\nFROM mido_edw_dev.sentiment_analysis.reviews_analysis\nwhere sentiment = 'Negative'\ngroup by item_name\norder by sum(review_count) desc\nlimit 10"
  }, {
    "name" : "c7719edf",
    "displayName" : "worst_product_by_theme",
    "query" : "SELECT t.item_name, count(t.item_id) Number_of_Themes\n FROM mido_edw_dev.sentiment_analysis.themes_analysis t\n inner join mido_edw_dev.sentiment_analysis.reviews_analysis r on t.item_id = r.item_id\n where r.sentiment = 'Negative'\n group by t.item_name\n order by Number_of_Themes desc\n limit 10"
  }, {
    "name" : "6adf7a21",
    "displayName" : "best_product_by_theme",
    "query" : "SELECT t.item_name, count(t.item_id) Number_of_Themes\n FROM mido_edw_dev.sentiment_analysis.themes_analysis t\n inner join mido_edw_dev.sentiment_analysis.reviews_analysis r on t.item_id = r.item_id\n where r.sentiment = 'Positive'\n group by t.item_name\n order by Number_of_Themes desc\n limit 10"
  }, {
    "name" : "63b29e61",
    "displayName" : "summary__worst_sentiment",
    "query" : "with cte_parsedlink as\n(select item_name, \nsentiment_score,\nsentiment,\nREPLACE(item_name, ' ', '%2520') AS parsed_product_name\nfrom mido_edw_dev.sentiment_analysis.reviews_analysis)\nselect item_name,\n round(avg(sentiment_score),1) Average_Sentiment,\n count(*) Total_Reviews,\nconcat('<a href=\"https://adb-984752964297111.11.azuredatabricks.net/dashboardsv3/01f01166a45b1e7196dc881b7a5d32ae/published/pages/e4f2954a?o=984752964297111&f_e4f2954a~6f005cfb=',parsed_product_name,'\" target=\"_blank\">',item_name,'</a>') Link\nfrom cte_parsedlink\nwhere sentiment_score is not null\nand sentiment = 'Negative'\ngroup by parsed_product_name, item_name\norder by Total_Reviews desc\nlimit 10"
  }, {
    "name" : "3a33ebf9",
    "displayName" : "product_sentiment_summary",
    "query" : "SELECT t.item_name, t.Key_theme, count(t.item_id) Number_of_Themes\n FROM mido_edw_dev.sentiment_analysis.themes_analysis t\n inner join mido_edw_dev.sentiment_analysis.reviews_analysis r on t.item_id = r.item_id\n where r.item_name =  :item \n group by t.item_name, t.Key_theme\n order by Number_of_Themes desc\n",
    "parameters" : [ {
      "displayName" : "item",
      "keyword" : "item",
      "dataType" : "STRING",
      "defaultSelection" : {
